<h1>Listing article_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Published</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @article_types.each do |article_type| %>
  <tr>
    <td><%= article_type.name %></td>
    <td><%= article_type.description %></td>
    <td><%= article_type.published %></td>
    <td><%= link_to 'Show', article_type %></td>
    <td><%= link_to 'Edit', edit_article_type_path(article_type) %></td>
    <td><%= link_to 'Destroy', article_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

